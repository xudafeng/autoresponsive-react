{
  "name": "autoresponsive-react",
  "version": "1.1.35",
  "description": "auto responsive grid layout library",
  "keywords": [
    "react",
    "responsive",
    "layout",
    "layout component",
    "ui component",
    "ui",
    "component"
  ],
  "files": [
    "dist/**/*.js"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/xudafeng/autoresponsive-react.git"
  },
  "scripts": {
    "dev": "webpack-dev-server --mode=development",
    "dev:test": "cross-env NODE_ENV=test npm run dev",
    "lint": "eslint --fix lib examples homepage test",
    "compile": "babel src/ --out-dir dist/ -D",
    "build": "webpack -p && npm run compile",
    "prepublish": "npm run build",
    "serve": "npm run dev:test &",
    "test": "macaca run -d ./test",
    "ci": "npm run lint && npm run serve && npm run test",
    "contributor": "git-contributor"
  },
  "dependencies": {
    "autoresponsive-core": "^1.0.1",
    "exenv": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "8",
    "bluebird": "^3.5.1",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.10.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "forkmeon.github.io": "1.1.15",
    "git-contributor": "1",
    "highlight.js": "^11.2.0",
    "husky": "^1.3.1",
    "json-loader": "^0.5.7",
    "jsx-loader": "^0.13.2",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "macaca-cli": "2",
    "macaca-electron": "11",
    "macaca-wd": "3",
    "marked": "^0.3.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-logo": "^1.0.15",
    "style-loader": "^0.21.0",
    "webpack": "^4.10.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.0.1",
    "webpack-dev-server": "^3.1.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
